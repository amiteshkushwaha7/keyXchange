{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\.js)",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}